{
  "name": "axe",
  "mode": "modules",
  "readme": "README.md",
  "out": "website/docs",
  "excludeExternals": true,
  "excludeNotExported": true,
  "excludePrivate": true,
  "includeDeclarations": false,
  "exclude": [
    "examples/**/*",
    "tests/**/*"
  ],
  "plugin": [
    "typedoc-plugin-external-module-name"
  ]
}
