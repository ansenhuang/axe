{
  "name": "axe",
  "version": "1.0.0",
  "description": "A series of libraries with typescript.",
  "author": "ansenhuang <ancheng1992@126.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "scripts": {
    "start": "webpack-dev-server --progress",
    "build:examples": "rimraf 'website/examples' && NODE_ENV=production webpack --progress",
    "build:lib": "rimraf 'packages/**/lib' && rollup -c",
    "build:unpkg": "lerna run unpkg",
    "prebuild": "yarn lint:css && yarn lint:lib",
    "build": "yarn build:lib && yarn build:unpkg",
    "pretest": "yarn lint:test",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "yarn lint:css && yarn lint:lib && yarn lint:test",
    "lint:css": "stylelint 'packages/**/*.css'",
    "lint:lib": "tslint --config tslint.lib.json 'packages/**/*.ts'",
    "lint:test": "tslint --config tslint.test.json 'packages/**/*.test.ts'",
    "fix": "yarn fix:css && yarn fix:lib && yarn fix:test",
    "fix:css": "yarn lint:css --fix",
    "fix:lib": "yarn lint:lib --fix",
    "fix:test": "yarn lint:test --fix",
    "docs": "typedoc --logger none",
    "commit": "git-cz",
    "codecov": "codecov",
    "predeploy": "yarn docs && yarn build:examples",
    "deploy": "node scripts/deploy-website.js",
    "prerelease": "yarn test && yarn build",
    "release": "lerna publish",
    "postrelease": "yarn deploy",
    "create-package": "node scripts/create-package.js"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.14",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "codecov": "^3.5.0",
    "commitizen": "^3.1.1",
    "core-js": "3",
    "css-loader": "^3.0.0",
    "cz-lerna-changelog": "^2.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob": "^7.1.4",
    "html-webpack-plugin": "^3.2.0",
    "inquirer": "^6.3.1",
    "jest": "^24.8.0",
    "lerna": "^3.15.0",
    "mini-css-extract-plugin": "^0.7.0",
    "postcss-advanced-variables": "^3.0.0",
    "postcss-calc": "^7.0.1",
    "postcss-cli": "^6.1.2",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.15.4",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.2",
    "rollup-plugin-postcss": "^2.0.3",
    "semantic-release": "^15.13.16",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "typescript": "^3.5.2",
    "url-loader": "^2.0.0",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-lerna-changelog"
    }
  }
}
